digraph AST {
  node [shape=box];
  node1 [label="root"];
  node2 [label="command: add(2)"];
  node1 -> node2;
  node3 [label="loop_start"];
  node1 -> node3;
  node4 [label="command: add(1)"];
  node3 -> node4;
  node5 [label="assignment: data[index] = 0"];
  node3 -> node5;
  node6 [label="assignment: data[index + 1] = temp"];
  node3 -> node6;
  node7 [label="loop_end"];
